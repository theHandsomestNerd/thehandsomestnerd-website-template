  "scripts": {
    "start": "NODE_ENV=production react-scripts start",
    "start:debug": "yarn build:dev && NODE_ENV=development serve -s build",
    "start:dev": "NODE_ENV=development react-scripts start",
    "build": "npm run build:esm && npm run build:cjs && npm run build:esm:babel && npm run build:cjs:babel",
    "build-web": "tsc && react-scripts build",
    "build-publish": "npm run build && npm publish",
    "build:assets": "cp src/logo.png dist/cjs && cp src/logo.png dist/esm",
    "build:fonts": "cp -r src/fonts dist/cjs && cp -r src/fonts dist/esm",
    "build:esm": "tsc",
    "build:cjs": "tsc --module commonjs --outDir build/cjs",
    "build:esm:babel": "babel build/esm --out-dir dist/esm",
    "build:cjs:babel": "babel --plugins @babel/plugin-transform-modules-commonjs build/cjs --out-dir dist/cjs",
    "build-npm": "rm -rf dist && NODE_ENV=production babel src --out-dir dist --copy-files",
    "build:dev": " NODE_ENV=development tsc --build && react-scripts build && cd functions && npm run build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "firebaseDeploy": "firebase deploy --only hosting",
    "storybook": "storybook dev",
    "build-storybook": "storybook build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
