{
  "name": "TheHandsomestNerdDigitalPortfolio",
  "version": "1.0.0",
  "description": "",
  "browserslist": [
    "> 0.5%, last 2 versions, not dead"
  ],
  "scripts": {
    "start": "npm run build:assets && parcel src/index.html",
    "build:assets": "mkdir -p dist && cp -r static/* dist/",
    "install-start": "npm run build:clean && npm install && npm start",
    "build:clean": "rimraf ./.parcel-cache ./dist",
    "build:babel": "babel build --out-dir dist",
    "build": "npm run build:clean && npm run build:assets && parcel build src/index.html --no-scope-hoist --dist-dir dist",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "@parcel/resolver-default": {
    "packageExports": true
  },
  "engines": {
    "node": "18"
  },
  "alias": {
    "react-dom/client": "react-dom/client.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@sanity/block-content-to-react": "^3.0.0",
    "@sanity/client": "^6.15.11",
    "@sanity/image-url": "^1.0.2",
    "dotenv": "^16.4.5",
    "parcel": "^2.12.0",
    "the-handsomestnerd-internal": "^0.1.182",
    "typescript": "^5.4.3"
  },
  "devDependencies": {
    "@babel/core": "^7.24.3",
    "@babel/plugin-transform-react-jsx": "^7.23.4",
    "@babel/preset-env": "^7.24.0",
    "@babel/preset-react": "^7.24.1",
    "@babel/preset-typescript": "^7.24.1",
    "@parcel/packager-raw-url": "^2.12.0",
    "@parcel/transformer-raw": "^2.12.0",
    "@parcel/transformer-webmanifest": "^2.12.0",
    "@types/node": "^20.11.30",
    "@types/react": "^18.2.73",
    "@types/react-dom": "^18.2.22",
    "buffer": "^6.0.3",
    "parcel-transformer-fonts": "^1.1.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
